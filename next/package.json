{
  "name": "next",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run pushSchemaToDB && next dev --turbopack",
    "pushSchemaToDB": "prisma format && prisma generate --schema prisma/schema.prisma && prisma db push --config prisma/prisma.config.ts --schema prisma/schema.prisma --accept-data-loss",
    "pullOldDB": "prisma db pull --config prismaOld/prisma.config.ts --schema prismaOld/schema.prisma && prisma generate --schema prismaOld/schema.prisma",
    "pushAndSeed": "prisma db push --config prisma/prisma.config.ts --schema prisma/schema.prisma --force-reset && prisma db seed --config prisma/prisma.config.ts",
    "bullmqDev": "NODE_ENV=development tsx watch bullmq/index.ts",
    "bullmq": "NODE_ENV=production tsx bullmq/index.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "7.1.0",
    "@fortawesome/free-brands-svg-icons": "7.1.0",
    "@fortawesome/free-regular-svg-icons": "7.1.0",
    "@fortawesome/free-solid-svg-icons": "7.1.0",
    "@fortawesome/react-fontawesome": "3.1.1",
    "@prisma/adapter-pg": "7.4.0",
    "@prisma/client": "7.4.0",
    "@tanstack/react-table": "8.21.3",
    "axios": "1.13.5",
    "bullmq": "5.66.0",
    "decimal.js": "10.6.0",
    "exceljs": "4.4.0",
    "jszip": "3.10.1",
    "minio": "8.0.6",
    "next": "16.1.6",
    "next-auth": "5.0.0-beta.30",
    "next-navigation-guard": "0.2.0",
    "prisma": "7.4.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-dropzone": "14.3.8",
    "tsx": "4.21.0",
    "validator": "13.15.23"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/validator": "13.15.2",
    "eslint": "^9.21.0",
    "eslint-config-next": "15.1.7",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "prettier": "3.7.4",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.3.1",
    "typescript": "^5"
  },
  "overrides": {
    "next-navigation-guard": {
      "next": "^14 || ^15 || ^16"
    }
  }
}
