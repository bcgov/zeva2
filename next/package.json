{
  "name": "next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run pushSchemaToDB && next dev --turbopack",
    "pushSchemaToDB": "prisma format && prisma db push --schema prisma/schema.prisma --accept-data-loss",
    "seedFromOldDB": "prisma db push --schema prisma/schema.prisma --force-reset && prisma db pull --schema prisma/schemaOld.prisma && prisma generate --schema prisma/schemaOld.prisma && prisma db seed",
    "pushAndSeed": "prisma db push --schema prisma/schema.prisma --force-reset --skip-generate && prisma db seed",
    "bullmqDev": "NODE_ENV=development tsx watch bullmq/index.ts",
    "bullmq": "NODE_ENV=production tsx bullmq/index.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "6.15.0",
    "@tanstack/react-table": "^8.21.2",
    "axios": "1.12.0",
    "bullmq": "5.41.8",
    "exceljs": "4.4.0",
    "jszip": "^3.10.1",
    "minio": "^8.0.5",
    "next": "15.5.2",
    "next-auth": "^5.0.0-beta.25",
    "prisma": "6.15.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "14.3.8",
    "react-icons": "^5.5.0",
    "tsx": "^4.20.3",
    "validator": "13.15.15"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/validator": "13.15.2",
    "eslint": "^9.21.0",
    "eslint-config-next": "15.1.7",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.3.1",
    "typescript": "^5"
  }
}
